{
  "manifest_version": 2,
  "name": "Bino le Dino",
  "description": "Informations sur le stream de binoledino",
  "version": "1.0",
  "icons": {
    "56": "icons/56x56/BinoWave.png"
  },
  "browser_action": {
    "default_icon": "icons/56x56/BinoCry.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["app.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
